{{#each authors}}
 <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">{{name}}</span>
          <p>{{username}}</p>
        </div>
        <div class="card-action">
          <a href="/super-admin/erase/{{_id}}">Borrar</a>
          <a href="/super-admin/modify/{{_id}}">Modificar</a>
        </div>
      </div>
    </div>
  </div>


{{/each}}

 <a href="/super-admin/create" class="waves-effect waves-light btn-large"><i class="material-icons left">person_add</i>Crear Nuevo</a>

